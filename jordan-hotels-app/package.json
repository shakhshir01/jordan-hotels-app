{
  "name": "jordan-hotels-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run generate-sitemaps",
    "prepare": "node scripts/husky-install.cjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "analyze": "vite build --sourcemap && echo 'open dist for analysis'",
    "audit": "npm audit --audit-level=moderate",
    "generate-sitemaps": "node scripts/generate-sitemaps.mjs",
    "deploy:aws": "bash lambda/deploy-and-seed.sh",
    "deploy:aws:windows": "powershell -ExecutionPolicy Bypass -File lambda\\deploy-and-seed.ps1"
  },
  "dependencies": {
    "@aws-amplify/analytics": "^6.5.15",
    "@aws-sdk/client-dynamodb": "^3.965.0",
    "@aws-sdk/lib-dynamodb": "^3.965.0",
    "@google/generative-ai": "^0.11.0",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^1.54.2",
    "@tanstack/react-virtual": "^3.13.17",
    "amazon-cognito-identity-js": "^6.3.16",
    "aws-amplify": "^5.3.29",
    "aws-rum-web": "^1.25.0",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "dotenv": "^16.4.5",
    "gh": "^2.5.0",
    "i18next": "^25.7.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.518.0",
    "node-fetch": "^3.3.2",
    "otplib": "^12.0.1",
    "p-limit": "^7.2.0",
    "qrcode": "^1.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.5.1",
    "react-lazy-load-image-component": "^1.6.3",
    "react-leaflet": "^4.2.1",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^7.11.0",
    "react-toastify": "^11.0.5",
    "sam": "^0.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.0",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.8.0",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^1.6.1"
  },
  "overrides": {
    "esbuild": "0.27.2",
    "micromatch": "4.0.8"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "npx eslint --fix",
      "npx prettier --write"
    ],
    "*.+(css|md|json)": [
      "npx prettier --write"
    ]
  }
}
